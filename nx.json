{
    "$schema": "./node_modules/nx/schemas/nx-schema.json",
    "targetDefaults": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["{projectRoot}/dist"],
            "cache": true
        },
        "dev": {
            "dependsOn": ["^dev"],
            "continuous": true
        },
        "check": {
            "inputs": [
                "default",
                "^default",
                "{projectRoot}/biome.json",
                "{workspaceRoot}/biome.json",
                { "externalDependencies": ["@biomejs/biome"] }
            ],
            "dependsOn": ["^check"],
            "cache": true
        }
    },
    "nxCloudId": "6900b1fc6c76005b050c8566"
}
